{
  "name": "schichtplan-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "bun": ">=1.0.0"
  },
  "trustedDependencies": [
    "bun-types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "format": "bunx --bun prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "bunx --bun prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "typecheck": "bunx --bun tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "start": "bunx --bun vite",
    "analyze": "bunx --bun vite-bundle-visualizer",
    "update-deps": "bun update --latest",
    "check-deps": "bunx --bun npm-check-updates",
    "install:clean": "rm -rf node_modules && bun install",
    "serve:prod": "bun build && bunx --bun serve dist",
    "check": "bun run typecheck && bun run lint && bun run format:check",
    "fix": "bun run lint:fix && bun run format",
    "prepare": "echo \"Skipping build during prepare. Run 'bun run build' manually to build the project.\"",
    "build:skip-typecheck": "bunx --bun vite build",
    "ci": "bun run clean && bun install && bun run check && bun run test && bun run build",
    "bundle:analyze": "bunx --bun vite build --mode analyze",
    "dev:host": "bunx --bun vite --host",
    "dev:debug": "bunx --bun vite --debug",
    "build:prod": "bunx --bun tsc && bunx --bun vite build --mode production",
    "build:staging": "bunx --bun tsc && bunx --bun vite build --mode staging",
    "postinstall": "echo \"Skipping TypeScript checks during installation. Run 'bun run typecheck' manually to check types.\"",
    "typecheck:ci": "bunx --bun tsc --noEmit",
    "precommit": "bun run lint && bun run test",
    "cache:clear": "rm -rf node_modules/.cache",
    "bun:upgrade": "bun upgrade",
    "bun:install": "bun install",
    "bun:add": "bun add",
    "bun:remove": "bun remove",
    "bun:update": "bun update",
    "bun:run": "bun run",
    "install:no-hooks": "bun install --no-scripts"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-aspect-ratio": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-hover-card": "^1.1.13",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-menubar": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-primitive": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toast": "^1.2.13",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/react-query": "^5.76.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/recharts": "^1.8.29",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.11.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.510.0",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-colorful": "^5.6.1",
    "react-day-picker": "8.10.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.56.3",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "^6.30.0",
    "recharts": "^2.15.3",
    "redux": "^5.0.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.4",
    "vaul": "^1.1.2",
    "zod": "^3.24.4",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/date-fns": "^2.6.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "bun-types": "latest",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^17.4.7",
    "jsdom": "^26.1.0",
    "msw": "^2.8.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.1",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5",
    "vite-bundle-visualizer": "^1.2.1"
  }
}