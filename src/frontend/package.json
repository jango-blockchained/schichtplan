{
  "name": "schichtplan-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "bun": ">=1.0.0"
  },
  "trustedDependencies": [
    "bun-types"
  ],
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun tsc && bunx --bun vite build",
    "lint": "bunx --bun eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "bunx --bun eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "bunx --bun vite preview",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "format": "bunx --bun prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "bunx --bun prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "typecheck": "bunx --bun tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "start": "bunx --bun vite",
    "analyze": "bunx --bun vite-bundle-visualizer",
    "update-deps": "bun update --latest",
    "check-deps": "bunx --bun npm-check-updates",
    "install:clean": "rm -rf node_modules && bun install",
    "serve:prod": "bun build && bunx --bun serve dist",
    "check": "bun run typecheck && bun run lint && bun run format:check",
    "fix": "bun run lint:fix && bun run format",
    "prepare": "echo \"Skipping build during prepare. Run 'bun run build' manually to build the project.\"",
    "build:skip-typecheck": "bunx --bun vite build",
    "ci": "bun run clean && bun install && bun run check && bun run test && bun run build",
    "bundle:analyze": "bunx --bun vite build --mode analyze",
    "dev:host": "bunx --bun vite --host",
    "dev:debug": "bunx --bun vite --debug",
    "build:prod": "bunx --bun tsc && bunx --bun vite build --mode production",
    "build:staging": "bunx --bun tsc && bunx --bun vite build --mode staging",
    "postinstall": "echo \"Skipping TypeScript checks during installation. Run 'bun run typecheck' manually to check types.\"",
    "typecheck:ci": "bunx --bun tsc --noEmit",
    "precommit": "bun run lint && bun run test",
    "cache:clear": "rm -rf node_modules/.cache",
    "bun:upgrade": "bun upgrade",
    "bun:install": "bun install",
    "bun:add": "bun add",
    "bun:remove": "bun remove",
    "bun:update": "bun update",
    "bun:run": "bun run",
    "install:no-hooks": "bun install --no-scripts"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tailwindcss/vite": "^4.0.9",
    "@tanstack/react-query": "^5.18.1",
    "@types/recharts": "^1.8.29",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.323.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.15.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "bun-types": "latest",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^17.1.4",
    "jsdom": "^26.0.0",
    "msw": "^2.2.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.1",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-bundle-visualizer": "^1.0.0"
  }
}